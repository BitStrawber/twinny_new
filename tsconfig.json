{
	"compilerOptions": {
	  "module": "ESNext",
	  "moduleResolution": "node",
	  "target": "ES2015",
	  "lib": ["ES2015", "dom"],
	  "jsx": "react-jsx",
	  "sourceMap": true,
	  "rootDir": "src",
	  "strict": true,
	  "noImplicitReturns": true,
	  "noFallthroughCasesInSwitch": true,
	  "noUnusedParameters": true,
	  "allowJs": true,
	  "esModuleInterop": true
	},
	"exclude": ["node_modules", ".vscode-test", "out", "scripts", "assets"]
  }
  